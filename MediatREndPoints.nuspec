<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>

    <id>MediatREndPoints</id>
    <version>1.0.3</version>
    <title>MediatREndPoints. MediatR + Minimal APIs </title>
    <authors>BabakTaremi</authors>
    <description>Easily Convert your MediatR request models to API endpoints </description>
    <summary>
    With a set of simple configurations, you can easily convert your MediatR endpoints to API endpoints using ASP NET Core minimal APIs
    </summary>
     <releaseNotes>
 Project is in early stages. Feel free to send pull requests to make it better at https://github.com/babaktaremi/MediatREndPoints/pulls
    </releaseNotes>
  

    <projectUrl>https://github.com/babaktaremi/MediatREndPoints</projectUrl>

     <dependencies>
    <dependency id="MediatR" version="12.0.1" />
    <dependency id="Microsoft.AspNetCore.OpenApi" version="7.0.2" />
  </dependencies>

<repository type="git" url="https://github.com/babaktaremi/MediatREndPoints" branch="master" />
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>MediatR, Mediator ASP NET CORE, Minimal API, REPR</tags>
    <icon>icon.png</icon>

  </metadata>

  <files>
    <file src=".template.config\icon.png" />
    <file src="README.md" />
  <file src="MediatREndPoints\bin\Release\net7.0\MediatREndPoints.dll" target="lib\net7.0" />
  <file src="MediatREndPoints\bin\Release\net7.0\MediatREndPoints.xml" target="lib\net7.0" />
    <file src=".\**" target="content" exclude="**\node_modules\**;**\.vs\**;**\.vscode\**;**\ClientApp\dist\**;**\wwwroot\dist\**;content\Directory.Build.*;**\.git\**;**\.github\**;**\CODE_OF_CONDUCT.md;**\LICENSE;**\README.md;**\nuget.exe;**/MediatREndPoints.Example/**" />
  </files>

</package>
